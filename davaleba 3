#include <iostream>

using namespace std;

int main(int argc, char** argv) {

    string y;
    string car[3] = { "BMW", "Lexus", "Audi" };
    int year[3] = {1, 2, 3};
    
    cout << "manqanis dasaxeleba: ";
    cin >> y;

    for (int i = 0;i < 3;i++) {
        if (y == car[i]) {
            cout << "gamoshvebis weli- " << year[i];
            
        } else {
        	cout << "manqana ver moidzebna ";
    
		}
    }

    return 0;
}
